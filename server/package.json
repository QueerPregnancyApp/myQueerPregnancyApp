{
  "name": "myQueerPregnancyApp",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently -k -n client,server -p \"[{name}]\" \"npm --prefix client run dev\" \"npm --prefix server run dev\"",
    "dev:client": "npm --prefix client run dev",
    "dev:server": "npm --prefix server run dev",
    "start": "npm --prefix server start",
    "fmt": "prettier -w \"**/*.{js,jsx,json,css,md}\""
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "prettier": "^3.3.3"
  }
}
{
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mongoose": "^6.10.0",
    "dotenv": "^16.0.3"
  },
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
